<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.example.missiond" >
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.CALL_PHONE"/>

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="MissionD"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="com.example.missiond.ScanQRcode"></activity>
    <activity android:name="com.example.missiond.RiderEndPayActivity"></activity>
    <activity android:name="com.example.missiond.RiderOnTripActivity" />
    <activity android:name="com.example.missiond.RiderWaitForPickUp" />
    <activity android:name="com.example.missiond.UserProfileActivity" />
    <activity android:name="com.example.missiond.DriverActivity" />
    <activity android:name="com.example.missiond.RiderActivity" />
    <activity android:name="com.example.missiond.RiderMakeRequestActivity" />
    <activity android:name="com.example.missiond.DriverPickeupRiderActivity"></activity>
    <activity android:name="com.example.missiond.DriverCompleteTripActivity"></activity>
    <activity android:name="com.example.missiond.DriverMakeOfferActivity"></activity>
    <activity android:name="com.example.missiond.DriverSearchRequestActivity"></activity>
    <meta-data
        android:name="com.google.android.gms.version"
        android:value="@integer/google_play_services_version" /> <!-- The API key for Google Maps-based APIs. -->
    <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value="@string/google_maps_key" />

    <activity android:name="com.example.missiond.MapActivity" />
    <activity android:name="com.example.missiond.RiderWaitForAcceptActivity" />
    <activity android:name="com.example.missiond.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>
</manifest>
